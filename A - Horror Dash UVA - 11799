#include<iostream>
using namespace std;

int main() {
    int T;
    cin >> T;

    for (int t = 1; t <= T; t++) {
        int n;
        cin >> n;
        int maxsp = 0;

        for (int i = 0; i < n; i++) {
            int speed;
            cin >> speed;
            maxsp = max(maxsp, speed);
        }

        cout << "Case " << t << ": " << maxsp << endl;
    }

    return 0;
}
